.display-box
    .display-box__logo
      svg(viewbox='0 0 188 188')
        g.author__img--box
          image(xlink:href='https://dummyimage.com/360x360/fff/aaa&text=Logo' height='100%' width='100%')
    .display-box__contents
      - let AOPSType = AOPSInfo.type.split(' ');
      h1(style="text-transform: uppercase;")
        | #{AOPSInfo.name} 
        br
        |  #{AOPSType[0]}
        br
        |  #{AOPSType[1]}
      p
        | #{AOPSInfo.displayMessage}